# Production Environment Variables
NODE_ENV=production

# Google Gemini API Key (must be set in production)
VITE_API_KEY=your_production_gemini_api_key_here

# Optional: Analytics and Monitoring
# VITE_ANALYTICS_ID=your_production_analytics_id
# VITE_SENTRY_DSN=your_production_sentry_dsn

# Optional: Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_MONITORING=true 